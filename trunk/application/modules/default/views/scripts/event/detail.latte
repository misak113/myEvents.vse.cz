				<form method="" action="" class="tm30">			<h1>{$event->name}</h1>				<div  class="row-fluid detailAct">														<div class="span9">										<p class="perex">                                            Začátek: <strong>{date('H:i', strtotime($event->timestart))}</strong> <span>|</span> Konec: <strong>{date('H:i', strtotime($event->timeend))}</strong> <span>|</span>	Místnost: <strong>{$event->location}</strong> <span>|</span>                                             Pořadatel:                                                 {foreach $organizations as $organization}                                                    <strong><a href="{$view->addHttp($view->cleanUrl($organization->website))}">{$organization->name}</a></strong> {if !$iterator->isLast()}|{/if}                                                {/foreach}                                        </p>					<table>			<tbody>					<td colspan="2">				<strong>Popis:				</strong>				<p>{$event->shortinfo}</p>                                <br />                                <p>{$event->longinfo}</p>				</td>				</tr>							<tr>			<th>				Kapacita: 				</th>				<td>					{$event->capacity}				</td>				</tr>				<tr>				<th>					Odkaz na FB: 					</th>				<td >					<a href="{$view->addHttp($view->cleanUrl($event->fburl))}">{$event->fburl}</a>				</td>			</tr>			<tr>			<th>				Sdílení: 					</th>			<td>			<a href="{$view->addHttp($view->cleanUrl($event->fburl))}" class="btn">Facebook</a>					<a href="#" class="btn">Twitter</a>					<a href="#" class="btn">Google+</a>	</td>			</tr>                        <tr>				<th>Sponzor:</th>				<td>                                    {foreach $sponsors as $sponsor}                                        <a href="{$view->addHttp($view->cleanUrl($sponsor->url))}">{$sponsor->name}</a> {if !$iterator->isLast()}|{/if}                                    {/foreach}				</td>			</tr>			</tbody></table>					</div>					<div class="span3">                                        {var $organization = $organizations->current()}					{if !$event->picture}                                            {if @$organization->logo}                                                <img src="{$view->baseUrl()}/img/logo/{$organization->logo}" alt="" />                                            {else}                                                <img src="{$view->baseUrl()}/img/action-screen.jpg" alt="" />                                            {/if}                                        {else}                                            <img src="{$view->baseUrl()}/img/event/{$event->picture}" alt="" />                                        {/if}					</div>					</div>		</form>				<a n:href="eventList" class="btn"><span class="icon-chevron-left"></span>Zpět na seznam akce</a>