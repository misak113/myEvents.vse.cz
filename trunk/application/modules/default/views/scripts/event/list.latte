{block content}<div class="row-fluid">    <div class="span3">       {control filter}    </div>    <div class="span9">                {foreach $eventDates as $date => $events}        {var $date = new DateTime($date)}        <div class="row-fluid row-action">            <div class="span2">                <div class="well"><h4>{$dayOfWeek[date('w', $date->getTimestamp())]}</h4><strong>{$date->format('d.m.')}</strong></div>            </div>            <div class="span10">                                {foreach $events as $event}                    {var $sponsors = $event->getSponsors()}                    {var $organizations = $event->getOrganizations()}                                    <div class="item row-fluid ">                            <div  class="row-fluid">                                <div class="span2">                                    <img src="{$view->baseUrl()}/img/action-screen.jpg" alt="" />                                </div>                                <div class="span10">                                    <h3>{$event->name}</h3>                                    <p class="perex">                                        Začátek: <strong>{date('H:i', strtotime($event->timestart))}</strong> <span>|</span> Konec: <strong>{date('H:i', strtotime($event->timeend))}</strong> <span>|</span>	Místnost: <strong>{$event->location}</strong> <span>|</span>                                         Pořadatel:                                         {foreach $organizations as $organization}                                        <strong><a href="{$view->addHttp($view->cleanUrl($organization->website))}">{$organization->name}</a></strong> {if !$iterator->isLast()}|{/if}                                        {/foreach}                                    </p>                                </div>                            </div>                        <div  class="row-fluid closed">                            <div class="span2 soc">                                <a href="{$view->addHttp($view->cleanUrl($event->fburl))}" class="btn">Facebook</a>                                <a href="#" class="btn">Twitter</a>                                <a href="#" class="btn">Google+</a>                            </div>                            <div class="span10">                                <table>                                    <tbody>                                        <tr>                                            <th colspan="2">                                                Popis:                                            </th>                                        </tr>                                        <tr>                                            <td colspan="2">                                                <p>{$event->shortinfo}</p>                                            </td>                                        </tr>                                        {ifset $event->capacity}                                            <tr>                                                <th>                                                    Kapacita:                                                </th>                                                <td>                                                    {$event->capacity}                                                </td>                                            </tr>                                        {/ifset}                                        {if count($sponsors) > 1}                                            <tr>                                                <th>Sponzor:</th>                                                <td>                                                    {foreach $sponsors as $sponsor}                                                        <a href="{$view->addHttp($view->cleanUrl($sponsor->url))}">{$sponsor->name}</a> {if !$iterator->isLast()}|{/if}                                                    {/foreach}                                                </td>                                            </tr>                                        {/if}                                        {if strlen($event->fburl) > 0}                                            <tr>                                                <th>                                                    Odkaz na FB:                                                </th>                                                <td >                                                    <a href="{$view->addHttp($view->cleanUrl($event->fburl))}">{$event->fburl}</a>                                                </td>                                            </tr>                                        {/if}                                    </tbody></table>                            </div></div>                            <div class="action">								<a href="#" class="btn detail-btn opened">Zobrazit detail</a>                                                                <a href="#" class="btn detail-btn closed">Skrýt detail</a>								<a href="<?php echo $view->url(                                                       array('id' => $event->event_id,'title' => $view->cleanUrl($event->name),                                                                       ),                                'event',true); ?>" class="btn">V novém okně</a></div>                    </div>                {/foreach}            </div>        </div>                {/foreach}        <a href="#" class="btn pull-right"><span class="iconn-align-left"></span>Další akce</a>    </div></div><script type="text/javascript">    $(function() {                var toggleEvent = function(scope) {            var item = $(scope.parents(".item")[0]);            if (item.length < 1) item = scope;            if (item.hasClass("curr"))                item.removeClass("curr");            else                item.addClass("curr");            item.find(".closed").toggle();            item.find(".opened").toggle();            return false;        }                $("body").on("click", ".item", function(e) {            console.log(e.target.tagName);            if ((e.target.tagName !== "A") && (e.target.parentElement.className.indexOf("action") == -1))                toggleEvent($(this));                    })                $("body").on("click", "a.detail-btn", function() {            toggleEvent($(this));            return false;        })          })</script>{/block}