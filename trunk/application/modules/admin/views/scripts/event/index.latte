<?php/** * @var Zend_View $view */?><a href="<?php echo $view->url(array(), "userLogout") ?>" class="logout btn fr">Odhlásit</a><a href="<?php echo $view->url(array(), 'editOrganization') ?>" class="edit-org btn fr">Upravit organizaci</a><a n:href="eventFbImport" class="edit-org btn fr btn-primary">FB import</a><h1>{$nazevOrganizace}</h1>		{if $events}    	<a href="<?php echo $view->url(array(), 'newEvent', true); ?>" class="btn btn-success"><span class="icon-plus"></span>Nová akce</a>			<table class="tm30 table table-striped">			<tr>				<th>Název</th>				<th>Datum</th>				<th>Editovat</th>				<th>Smazat</th>			</tr>			<tr n:foreach="$events as $event">				<td>{$event->name}</td>                                <td>{$event->timestart|date:'%d.%m.%Y'}</td>				<td><a href="<?php echo $view->url(                                       array('id' => $event->event_id),                                       'editEvent',                                       true);?>"                                       class="btn"><span class="icon-pencil"></span>editovat</a></td>				<td>                                    <form action="<?php echo $view->url(                                          array('module' => 'admin',                                            'controller' => 'event',                                            'action' => 'delete',                                            'id' => $event->event_id),                                          'default',                                          true); ?>"                                        method="post">                                        <input type="submit" value="smazat" onclick="return confirm('Opravdu si přejete akci smazat?')" />                                    </form>                                </td>			</tr>			</table>		{/if}