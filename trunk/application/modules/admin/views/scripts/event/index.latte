<?php/*** @var Zend_View $view*/?><div class="submenu">	<ul class="fr">		{*<li><a n:href="eventFbExport" class="btn gr">FB export</a></li>*}		<li><a n:href="eventFbImport" class="btn gr">FB import</a></li>		<li><a href="<?php echo $view->url(array(), 'editOrganization') ?>" class="btn gr">Upravit organizaci</a></li>		<li><a href="<?php echo $view->url(array(), "userLogout") ?>" class="btn gr">Odhlásit</a></li>	</ul></div><div class="background">	<div n:if="!empty($organizations)" n:foreach="$organizations as $organization" class="inside listA">				<h1>{$organization->name}</h1>		{var $events = $organization->getEvents()}{if $events}    <a href="<?php echo $view->url(array('organization_id' => $organization->getId()), 'newEvent', true); ?>" class="btn new"><span class="icon-plus"></span>Nová akce</a>    <table class=" table table-striped">        <tr>            <th>Název</th>            <th>Datum</th>            <th>Viditelnost</th>            <th>Škola</th>            <th></th>            <th></th>        </tr>        <tr n:foreach="$events as $event">            <td>                {$event->name}            </td>            <td>{$event->timestart|date:'%d.%m.%Y'}</td>            <td>                {if $event->public}                <form action="<?php echo $view->url(                      array('module' => 'admin',                      'controller' => 'event',                      'action' => 'publish',                      'id' => $event->event_id,                      'public' => 0),                      'default',                      true); ?>" method="post">                    <input type="submit" value="Skrýt" onclick="return confirm('Opravdu si přejete skrýt akci na hlavní stránce?')"                           class="btn"/>                </form>                {else}                <form action="<?php echo $view->url(                      array('module' => 'admin',                      'controller' => 'event',                      'action' => 'publish',                      'id' => $event->event_id,                      'public' => 1),                      'default',                      true); ?>" method="post">                    <input type="submit" value="Publikovat" onclick="return confirm('Opravdu si přejete akci publikovat? Na stránce my-events.cz se akce zobrazí ihned i bez schválení školou.')"                            class="btn"/>                </form>                {/if}            </td>            <td>                {if !$event->ready_to_approve}                <form action="<?php echo $view->url(                      array('module' => 'admin',                      'controller' => 'approve',                      'action' => 'readyToApprove',                      'id' => $event->event_id,                      'ready' => 1),                      'default',                      true); ?>" method="post">                    <input type="submit" value="Schválit a zkontrolovat" onclick="return confirm('Opravdu si přejete odeslat akci škole ke schválení? Akci již nebude možné editovat.')"                           class="btn"/>                </form>                {else}                    {if $event->approved != null && $event->controlled != null}                        <span class="label label-success">Schváleno</span>                    {else}                        <span class="label label-info">Čeká na schválení</span>                    {/if}                {/if}            </td>            <td>                {if !$event->ready_to_approve}                <a href="<?php echo $view->url(                   array('id' => $event->event_id),                   'editEvent',                   true);?>"                   class="btn bl">editovat</a>                {/if}            </td>            <td>                <form action="<?php echo $view->url(                      array('module' => 'admin',                      'controller' => 'event',                      'action' => 'delete',                      'id' => $event->event_id),                      'default',                      true); ?>"                      method="post">                    <input type="submit" value="smazat" onclick="return confirm('Opravdu si přejete akci smazat?')"                            class="btn rd"/>                </form>            </td>        </tr>    </table>	{else}		<div>{_'Nejsou k dispozici žádné akce'}</div>{/if}</div>	<div n:if="empty($organizations)">		{_'Nejste členem žádné organizace'}	</div></div>